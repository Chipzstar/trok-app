{
	"name": "trok-app",
	"version": "0.0.0",
	"license": "MIT",
	"cacheDirectories": [
		"/apps/trok-dashboard/.next/cache"
	],
	"engines": {
		"npm": ">=8.0.0 <9.0.0",
		"node": ">=16.0.0 <17.0.0"
	},
	"scripts": {
		"start": "nx serve",
		"build": "nx build",
		"test": "nx test",
		"nx:backend:serve": "doppler run -p backend -c loc -- nx serve trok-backend",
		"nx:backend:serve:ci": "nx serve trok-backend --no-dte",
		"nx:backend:lint": "nx lint trok-backend",
		"nx:backend:build": "nx build trok-backend",
		"nx:backend:build:prod": "nx build trok-backend --no-cloud",
		"nx:backend:serve:prod": "nx serve trok-backend --prod",
		"nx:backend:serve:prod:verbose": "nx serve trok-backend --prod --verbose",
		"nx:dashboard:lint": "nx lint trok-dashboard",
		"nx:dashboard:serve": "doppler run -p admin-dashboard -c loc -- nx serve trok-dashboard",
		"nx:dashboard:build": "nx build trok-dashboard --no-cloud",
		"nx:dashboard:serve:prod": "nx serve trok-dashboard --prod --hostname 0.0.0.0",
		"nx:dashboard:serve:ci": "nx serve trok-dashboard --prod",
		"nx:dashboard:test:local": "doppler run -p admin-dashboard -c loc -- nx build trok-dashboard && doppler run -p admin-dashboard -c loc -- nx serve trok-dashboard --prod --port 4201",
		"nx:dashboard:test:dev": "doppler run -p admin-dashboard -c dev -- nx build trok-dashboard && doppler run -p admin-dashboard -c dev -- nx serve trok-dashboard --prod --port 4201",
		"nx:dashboard:test:dev:no-cloud": "nx build trok-dashboard --no-cloud && nx serve trok-dashboard --prod --port 4201 --no-cloud",
		"nx:dashboard:e2e": "start-server-and-test \"yarn run nx:backend:serve\" http://localhost:3333/ping \"nx e2e trok-dashboard-e2e\"",
		"nx:dashboard:e2e:lint": "nx lint trok-dashboard-e2e",
		"nx:dashboard:e2e:spec": "start-server-and-test nx:backend:serve http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=spec'",
		"nx:dashboard:e2e:spec:headless": "start-server-and-test nx:backend:serve http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=spec-headless'",
		"nx:dashboard:e2e:spec:chrome": "start-server-and-test nx:backend:serve http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=spec-chrome'",
		"nx:dashboard:e2e:spec:firefox": "start-server-and-test nx:backend:serve http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=spec-firefox'",
		"nx:dashboard:e2e:spec:edge": "start-server-and-test nx:backend:serve http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=spec-edge'",
		"nx:dashboard:e2e:spec:safari": "start-server-and-test nx:backend:serve http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=spec-safari'",
		"nx:dashboard:e2e:spec:record": "start-server-and-test 'nx serve trok-backend' http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=spec-record'",
		"nx:dashboard:e2e:watch": "start-server-and-test nx:backend:serve http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=watch'",
		"nx:dashboard:e2e:record": "start-server-and-test 'nx serve trok-backend' http://localhost:3333/ping 'nx e2e trok-dashboard-e2e --configuration=record'",
		"nx:analytics:serve": "doppler run -p admin-dashboard -c loc -- nx serve trok-analytics",
		"nx:analytics:serve:prod": "nx serve trok-analytics --prod --hostname 0.0.0.0",
		"nx:analytics:test:local": "doppler run -p admin-dashboard -c loc -- nx build trok-analytics && doppler run -p admin-dashboard -c loc -- nx serve trok-analytics --prod",
		"nx:analytics:build": "nx build trok-analytics --no-cloud",
		"nx:cache-clean": "nx reset",
		"prisma:generate": "npx prismerge -i prismerge.json && npx prisma generate",
		"prisma:studio": "npx prisma studio",
		"prisma:db:push": "npx prisma db push",
		"create-env-file:backend": "doppler secrets download --no-file --format env > ./apps/trok-backend/.env -p backend -c loc",
		"create-env-file:dashboard": "doppler secrets download --no-file --format env > ./apps/trok-dashboard/.env -p admin-dashboard -c loc",
		"stripe-local-webhook": "stripe listen --forward-to localhost:3333/server/stripe/webhook",
		"circleci-validate": "circleci config validate"
	},
