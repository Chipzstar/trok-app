{
	"name": "trok-dashboard-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/trok-dashboard-e2e/src",
	"projectType": "application",
	"targets": {
		"e2e": {
			"executor": "@nrwl/cypress:cypress",
			"options": {
				"cypressConfig": "apps/trok-dashboard-e2e/cypress.config.ts",
				"devServerTarget": "trok-dashboard:serve:development",
				"testingType": "e2e"
			},
			"configurations": {
				"spec": {
					"spec": "apps/trok-dashboard-e2e/src/e2e/1-drivers/*.ts",
					"exit": false
				},
				"spec-headless": {
					"spec": "apps/trok-dashboard-e2e/src/e2e/1-drivers/*.ts"
				},
				"spec-chrome": {
					"spec": "apps/trok-dashboard-e2e/src/e2e/1-drivers/*.ts",
					"browser": "chrome"
				},
				"spec-firefox": {
					"spec": "apps/trok-dashboard-e2e/src/e2e/1-drivers/*.ts",
					"browser": "firefox"
				},
				"spec-edge": {
					"spec": "apps/trok-dashboard-e2e/src/e2e/1-drivers/*.ts",
					"browser": "edge"
				},
				"spec-safari": {
					"spec": "apps/trok-dashboard-e2e/src/e2e/1-drivers/*.ts",
					"browser": "webkit"
				},
				"spec-record": {
					"spec": "apps/trok-dashboard-e2e/src/e2e/1-drivers/*.ts",
					"env": {
						"API_BASE_URL": "http://localhost:3333",
						"BASE_URL": "http://localhost:4200"
					},
					"parallel": true,
					"record": true,
					"key": "bf0463d1-6ab9-4691-91fc-5f8578b7e72f"
				},
				"watch": {
					"browser": "electron",
					"env": {
						"API_BASE_URL": "http://localhost:3333",
						"BASE_URL": "http://localhost:4200"
					},
					"watch": true
				},
				"record": {
					"env": {
						"API_BASE_URL": "http://localhost:3333",
						"BASE_URL": "http://localhost:4200"
					},
					"parallel": true,
					"record": true,
					"key": "bf0463d1-6ab9-4691-91fc-5f8578b7e72f"
				},
				"development": {
					"env": {
						"API_BASE_URL": "https://trok-api-dev.onrender.com",
						"BASE_URL": "https://trok-app-dev.onrender.com"
					}
				},
				"production": {
					"devServerTarget": "trok-dashboard:serve:production",
					"env": {
						"API_BASE_URL": "https://trok-api.onrender.com",
						"BASE_URL": "https://app.trok.co"
					}
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"apps/trok-dashboard-e2e/**/*.{js,ts}"
				]
			}
		}
	},
	"tags": [],
	"implicitDependencies": [
		"trok-dashboard",
		"trok-backend"
	]
}
