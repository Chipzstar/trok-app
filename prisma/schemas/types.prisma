type StripeInfo {
    accountId String
    bankAccount BankAccount
}

type BankAccount {
    id String
    fingerprint String
    account_holder_name String
    account_number String
    sort_code String
    currency String
    status String
}

type Address {
    line1 String
    line2 String?
    city String
    postcode String
    region String
    country String @default("GB")
}

type BusinessInfo {
    legal_name String
    weekly_fuel_spend Int
    business_type String
    business_crn String
    merchant_category_code String
    business_url String
    num_vehicles Int
}