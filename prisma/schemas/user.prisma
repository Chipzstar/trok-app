model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  createdAt  DateTime   @default(now())
  updatedAt DateTime @updatedAt
  email String  @unique
  full_name String
  firstname  String
  lastname  String
  phone String
  referral_code String?
  password String
  address Address?
  business BusinessInfo?
  stripe StripeInfo?
}